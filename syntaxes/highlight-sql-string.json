{
  "fileTypes": [
    "py",
    "go",
    "js",
    "jsx",
    "ts",
    "tsx",
    "vue",
    "svelte",
    "java",
    "rb",
    "cs",
    "rs",
    "php"
  ],
  "injectionSelector": [
    "text.html.php",
    "L:source -comment -string"
  ],
  "patterns": [
    {
      "comment": "Rust multi-line raw strings",
      "begin": "(b?r)(#*)(\")(--sql)",
      "beginCaptures": {
        "1": {
          "name": "string.quoted.byte.raw.rust"
        },
        "2": {
          "name": "string.raw.rust"
        },
        "3": {
          "name": "string.rust"
        },
        "4": {
          "name": "comment.sql"
        }
      },
      "end": "(\")(\\2)",
      "endCaptures": {
        "1": {
          "name": "string.rust"
        },
        "2": {
          "name": "string.raw.rust"
        }
      },
      "patterns": [
        {
          "include": "source.sql"
        }
      ]
    },
    {
      "begin": "(`)((--sql)|SELECT |INSERT INTO |DELETE |UPDATE |CREATE TABLE)",
      "beginCaptures": {
        "2": {
          "name": "keyword"
        }
      },
      "end": "(`)",
      "patterns": [
        {
          "include": "source.sql"
        }
      ]
    },
    {
      "begin": "(\")((--sql)|SELECT |INSERT INTO |DELETE |UPDATE |CREATE TABLE)",
      "beginCaptures": {
        "2": {
          "name": "keyword"
        }
      },
      "end": "(\")",
      "patterns": [
        {
          "include": "source.sql"
        }
      ]
    },
    {
      "begin": "(\"\"\")((--sql)|SELECT |INSERT INTO |DELETE |UPDATE |CREATE TABLE)",
      "beginCaptures": {
        "2": {
          "name": "keyword"
        }
      },
      "end": "(\"\"\")",
      "patterns": [
        {
          "include": "source.sql"
        }
      ]
    }
  ],
  "scopeName": "inline-sql.injection"
}
